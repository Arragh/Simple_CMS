<!DOCTYPE html>

<html>
<head>

    <!--Metatags-->
    @await Html.PartialAsync("MetatagsPartial")

</head>
<body bgcolor="darkgrey">

    <!--Header-->
    @await Html.PartialAsync("HeaderPartial")

    <div align="center">
        <table border="1" width="1200" height="800" bgcolor="lightblue" cellpadding="0" cellspacing="0">
            <tr>
                <td valign="top">@RenderBody()</td>
            </tr>
        </table>
    </div>

    <!--Bottom-->
    @await Html.PartialAsync("BottomPartial")

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)

</body>
</html>